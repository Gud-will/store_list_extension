{
    "manifest_version": 3,
    "name": "Link and Note Manager",
    "version": "1.3",
    "description": "Store and manage links and notes in different groups.",
    "permissions": ["storage", "contextMenus"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  